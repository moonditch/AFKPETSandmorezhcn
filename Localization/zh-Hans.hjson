			AFKPetsConfiguration: {
				PetLiteMode: {
					Tooltip: 启用后，禁用宠物掉落
					Label: "[i:Cog] 禁用宠物"
				}

				UltraLiteMode: {
					Tooltip: 启用后，禁用任何使用的来自其它游戏的贴图
					Label: "[i:Cog] 纯净贴图模式"
				}

				BossLiteMode: {
					Tooltip: 启用后，本模组的任何Boss都无法生成，它们的生成物也无法获得
					Label: "[i:Cog] 禁用AFK宠物模组的Boss"
				}

				WingsLiteMode: {
					Tooltip: 启用后，本模组的翅膀将被卸载。如果你装了很多其它模组，翅膀的总数超过127个导致游戏崩溃，启用此选项会有帮助
					Label: "[i:AngelWings] 禁用翅膀"
				}

				LiteMode: {
					Tooltip: 启用后，本模组除了宠物之外的其它内容将无法获得
					Label: "[i:Cog] 禁用额外内容s"
				}

				TrinketOffsetY: {
					Tooltip: 从上到下调整AFK密藏栏的垂直位置
					Label: "[i:Book] 调整AFK密藏栏在屏幕上的垂直显示位置"
				}

				TrinketOffsetX: {
					Tooltip: 从左到右调整AFK密藏栏的水平位置
					Label: "[i:Book] 调整AFK密藏栏在屏幕上的水平显示位置"
				}

				TrinketSlotHide: {
					Tooltip: 启用后，禁用背包下方显示的AFK密藏栏
					Label: "[i:Book] 隐藏AFK密藏栏"
